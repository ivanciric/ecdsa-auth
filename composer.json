{
    "name": "ivanciric/ecdsa-auth",
    "description": "ECDSA based authentication for Laravel/Dingo API",
    "type": "library",
    "require": {
        "dingo/api": "2.0.0-alpha1",
        "simplito/elliptic-php": "1.0.4",
        "kornrunner/keccak": "^1.0"
    },
    "require-dev": {},
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Ciric",
            "email": "info@yoshi.tech"
        }
    ],
    "autoload": {
        "psr-4": {
            "ivanciric\\EcdsaAuth\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ivanciric\\EcdsaAuth\\ServiceProvider"
            ],
            "aliases": {
                "EcdsaAuth": "ivanciric\\EcdsaAuth\\AuthenticatorFacade"
            }
        }
    }
}
